    "cross-env": "7.0.3",
    "firebase-admin": "^11.5.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "3.12.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
